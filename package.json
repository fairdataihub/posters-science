    "sanitize-html": "^2.14.0",
    "vue": "latest",
    "vue3-lottie": "^3.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@faker-js/faker": "^9.4.0",
    "@nuxt/devtools": "2.5.0",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/ui": "^4.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@prisma/client": "^6.16.3",
    "@stylistic/eslint-plugin": "^5.4.0",
