    "@types/nodemailer": "^6.4.17",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/parser": "^8.45.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs-nuxt": "2.1.11",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
